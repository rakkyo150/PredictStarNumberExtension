{
    "name": "PredictStarNumberExtension",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "Google Chrome Extension version of PredictStarNumber",
    "content_scripts": [{
        "matches": ["https://beatsaver.com/*"],
        "js": [
        "BeatSaver.js"
        ],
        "run_at": "document_end",
        "all_frames": true
    },
    {
        "matches": ["https://scoresaber.com/*"],
        "js": [
        "ScoreSaber.js"
        ],
        "run_at": "document_end",
        "all_frames": true
    }]
}